{
   "background": {
      "scripts": [ "online-service/js/musicplayer.background.min.js" ]
   },
   "commands": {
      "next": {
         "description": "Play next track",
         "global": true,
         "suggested_key": {
            "default": "MediaNextTrack"
         }
      },
      "play": {
         "description": "Toggle play & pause",
         "global": true,
         "suggested_key": {
            "default": "MediaPlayPause"
         }
      },
      "prev": {
         "description": "Play previous track",
         "global": true,
         "suggested_key": {
            "default": "MediaPrevTrack"
         }
      },
      "toggle-musicplayer": {
         "description": "Toggle Sidebar Music Player",
         "suggested_key": {
            "default": "Alt+Shift+M",
            "mac": "Alt+Shift+M",
            "windows": "Alt+Shift+M"
         }
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "exclude_matches": [ "*://playerui.music.naver.com/logout.html", "*://www.genie.co.kr/player/shareProcessV2*" ],
      "js": [ "online-service/js/musicplayer.player.min.js" ],
      "matches": [ "*://playerui.music.naver.com/*", "*://music.bugs.co.kr/newPlayer*", "*://www.mnet.com/player/*", "*://www.genie.co.kr/player/*", "*://www.genie.co.kr/mvPlayer/*" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "default_locale": "ko",
   "description": "__MSG_APP_DESCRIPTION__",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "incognito": "split",
   "key": "",
   "manifest_version": 2,
   "minimum_whale_version": "1.0.37.12",
   "name": "__MSG_APP_NAME__",
   "permissions": [ "webRequest", "webRequestBlocking", "storage", "cookies", "*://*.genie.co.kr/*", "tabs", "http://*/*", "https://*/*", "unlimitedStorage", "\u003Call_urls>" ],
   "sidebar_action": {
      "default_icon": {
         "16": "icons/icon16.png"
      },
      "default_page": "index.html",
      "default_title": "__MSG_APP_NAME__",
      "mobile_user_agent": false,
      "use_navigation_bar": false
   },
   "update_url": "https://store.whale.naver.com/update/whx",
   "version": "2.4.73",
   "web_accessible_resources": [ "online-service/img/*.png", "online-service/img/*.svg" ],
   "whale_extension": true
}
